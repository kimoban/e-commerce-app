{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react", "react-native"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-unused-vars": "warn",
    "react/prop-types": "off"
  }
}
