{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "framework": null,
  "redirects": [
    { "source": "/(.*)", "destination": "/$1", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    }
  ]
}
